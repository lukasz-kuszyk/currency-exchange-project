{
  "name": "nauta/currency-exchange-project",
  "type": "project",
  "autoload": {
    "psr-4": {
      "Nauta\\CurrencyExchangeProject\\Domain\\": "src/Domain"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Nauta\\CurrencyExchangeProject\\Unit\\": "tests/Unit"
    }
  },
  "require": {
    "php": "^8.2",
    "brick/math": "^0.12.1",
    "webmozart/assert": "^1.11"
  },
  "require-dev": {
    "behat/behat": "^3.14",
    "friendsofphp/php-cs-fixer": "^3.58",
    "phpstan/extension-installer": "^1.4",
    "phpstan/phpstan": "^1.11",
    "phpstan/phpstan-strict-rules": "^1.6",
    "phpunit/phpunit": "^11"
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "phpstan/extension-installer": true
    }
  },
  "extra": {
    "phpstan": {
      "includes": ["extension.neon"]
    }
  }
}
